{
  "name": "securemedz",
  "version": "1.0.0",
  "description": "securemedz wordpress theme packages",
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf build/*",
    "svg": "svgr -d src/js/icons src/icons",
    "lint": "eslint src/js/*",
    "lint:fix": "eslint --fix src/js/*",
    "lint-css": "stylelint 'src/sass/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint-css:fix && npm run lint:fix && npm run prod"
  },
  "keywords": [
    "wordpress",
    "themes"
  ],
  "author": "Ravi Makwana",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@svgr/cli": "^6.2.1",
    "@wordpress/base-styles": "^4.4.0",
    "@wordpress/block-editor": "^8.6.0",
    "@wordpress/blocks": "^11.6.0",
    "@wordpress/components": "^19.9.0",
    "@wordpress/compose": "^5.5.0",
    "@wordpress/data": "^6.7.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.4.1",
    "@wordpress/element": "^4.5.0",
    "@wordpress/eslint-plugin": "^12.1.0",
    "@wordpress/hooks": "^3.7.0",
    "@wordpress/i18n": "^4.7.0",
    "@wordpress/icons": "^8.3.0",
    "@wordpress/server-side-render": "^3.5.0",
    "@wordpress/stylelint-config": "^20.0.2",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.1.7",
    "eslint": "^8.14.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "glob-all": "^3.3.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "sass-mq": "^6.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "uglify-js": "^3.15.4"
  }
}
